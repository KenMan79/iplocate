{
  "name": "iplocate-backend",
  "version": "0.0.1",
  "description": "Backend for iplocate tool",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"npm:build:watch\" \"nodemon\"",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput --pretty",
    "start": "node dist/index.js"
  },
  "author": "Jeff Hackshaw <jeffrey.hackshaw@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fastify": "^3.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
